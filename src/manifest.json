{
	"manifest_version": 2,
	"name": "proofskip",
	"version": "0.9",
	"description": "Decode and bypass proofpoint links automatically",

	"browser_specific_settings": {
		"gecko": {
			"id": "{1597d2f1-3293-4d96-8521-0b143c20d3dc}"
		}
	},

	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"contextMenus",
		"activeTab",
		"clipboardWrite",
		"*://urldefense.com/*",
		"*://urldefense.proofpoint.com/*"
	],

	"background": {
		"scripts": [
			"browser-polyfill.js",
			"he.js",
			"proofskip.js"
		]
	},

	"web_accessible_resources": [
		"error.html",
		"error.js"
	]
}
